@{
    ViewBag.Title = "Services Detail";
}
@model DONATE_TO_SAVE.Models.Service

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title></title>
</head>
<body>
    <div class="container">
        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    Servies
                </h1>
                <ol class="breadcrumb">
                    <li>
                        <a href="#">Home</a>
                    </li>
                    <li class="active">Service</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->
        <!-- Content Row -->
        <div class="row">
            <!-- Map Column -->

            <div class="col-md-6">
                <p>
                    <font size="5">
                        <i class="fa fa-home"></i>
                        <abbr title="Home">Service Name</abbr>: @Model.Description
                    </font>
                </p>

                <p>
                    <font size="5">
                        <i class="fa fa-pencil-square-o"></i>
                        <abbr title="Name">Details</abbr>:<br/> @Model.Complete_Explanation
                    </font>
                </p>

            </div>
        </div>
        <!-- /.row -->
    </div>
</body>
</html>